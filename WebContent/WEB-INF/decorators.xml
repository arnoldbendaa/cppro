<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/decorators">
    <decorator name="main" page="main.jsp">
		<pattern>*</pattern>
    </decorator>
    <decorator name="popup" page="popupMain.jsp">
        <pattern>*/popup*</pattern>
        <pattern>*viewAttachment.jsp*</pattern>
    </decorator>
    <decorator name="none" page="noDecoration.jsp">
        <pattern>*/popup*Close.jsp</pattern>
	</decorator>
    <decorator name="inLine" page="inLine.jsp">
		<pattern>*/inline/*</pattern>
	</decorator>
	<decorator name="basic" page="basicMain.jsp" >
		<pattern>*/full*.jsp</pattern>
		<!--<pattern>*/fullListPopup*</pattern>-->
	</decorator>
	<decorator name="test" page="test.jsp">
        <pattern>*/test/*</pattern>
    </decorator>
	<decorator name="gadget" page="gadget.jsp">
        <pattern>*/gadget/*</pattern>
    </decorator>
    <decorator name="budgetIframe" page="budgetIframe.jsp">
		<pattern>*/budgetCycleStatus.jsp</pattern>
    </decorator>
</decorators>